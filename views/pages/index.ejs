<?xml version="1.0" xmlns:atom="http://www.w3.org/2005/Atom" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
  <title>W3Schools Home Page</title>
  <link>https://www.w3schools.com</link>
  <description>Free web building tutorials</description>
  <% rssItems.forEach(function(rssItem) { %>
   <item>
    <title><%= rssItem.title %></title>
    <link><%= rssItem.link %></link>
    <description><%= rssItem.description %></description>
    <pubDate><%= rssItem.pubDate %></pubDate>
    <media:content url="<%= rssItem.mediaContent %>"/>
   </item>
  <% }); %>
</channel>
</rss>
  
